          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberOfAlerts">
          <DataField>NumberOfAlerts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
