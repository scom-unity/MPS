  <DataSets>
    <DataSet Name="OPMGRDW">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AlertName">
            <Value>=Parameters!AlertName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select DisplayName,AlertName,Count(AlertName)[NumberOfAlerts]
 from Alert.vAlertResolutionState ars 
inner join Alert.vAlertDetail adt on ars.alertguid = adt.alertguid 
inner join Alert.vAlert alt on ars.alertguid = alt.alertguid
Inner Join vManagedEntity me on alt.ManagedEntityRowID = me.ManagedEntityRowID
Where RaisedDateTime &gt;= DateAdd(Hour,-96,GetUTCDate())
And ResolutionState &lt; 255
And AlertName = @AlertName
Group By DisplayName,AlertName
Order By Count(AlertName) Desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AlertName">
