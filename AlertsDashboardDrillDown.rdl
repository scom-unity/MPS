  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SeverityFromReport">
            <Value>=Parameters!SeverityFromReport.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTime">
            <Value>=Parameters!DateTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Severity int

IF @SeverityFromReport = 'INFORMATIONAL' 
Begin
Set @Severity = 0
End

IF @SeverityFromReport = 'WARNING'
Begin
Set @Severity = 1
End

IF @SeverityFromReport = 'CRITICAL' 
Begin
Set @Severity = 2
End

SELECT  SUBSTRING(path,0,PATINDEX('%;%',Path  + ';')) AS PATH,DisplayName,severity,AlertName,[NumberOfAlerts] FROM (

select ISNULL(Path,DisplayName)[path],DisplayName,severity,AlertName,Count(AlertName)[NumberOfAlerts]
 from Alert.vAlertResolutionState ars 
inner join Alert.vAlertDetail adt on ars.alertguid = adt.alertguid 
inner join Alert.vAlert alt on ars.alertguid = alt.alertguid
Inner Join vManagedEntity me on alt.ManagedEntityRowID = me.ManagedEntityRowID
Where Convert(varchar(13),RaisedDateTime +RIGHT(SYSDATETIMEOFFSET(), 5),20) = @DateTime  and ResolutionState &lt; 255 And Severity = @Severity
Group By Path,DisplayName,Severity,AlertName
) AS T1
Group By Path,DisplayName,Severity,AlertName,[NumberOfAlerts]
Order By Count(AlertName) Desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Severity">
          <DataField>severity</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="PATH">
          <DataField>PATH</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AlertName">
          <DataField>AlertName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberOfAlerts">
          <DataField>NumberOfAlerts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
