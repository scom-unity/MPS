    <DataSet Name="Trend_Analysis">
      <Query>
        <DataSourceName>DataWarehouseMain</DataSourceName>
        <CommandText>select Convert(varchar(13),RaisedDateTime +RIGHT(SYSDATETIMEOFFSET(), 5),20)[RaisedDateTime], 

   'Severity' = Case
	When Severity = 0 Then 'INFORMATIONAL'
	When Severity = 1 Then 'WARNING'
	When Severity = 2 Then 'CRITICAL'
	Else 'Undetermined'
	End,
	Count(*)[NumberOfAlerts] 
from Alert.vAlertResolutionState ars 
inner join Alert.vAlertDetail adt on ars.alertguid = adt.alertguid 
inner join Alert.vAlert alt on ars.alertguid = alt.alertguid
Inner Join vManagedEntity me on alt.ManagedEntityRowID = me.ManagedEntityRowID
Where RaisedDateTime &gt;= DateAdd(Hour,-120,GetUTCDate())
And ResolutionState &lt; 255
Group By Convert(varchar(13),RaisedDateTime +RIGHT(SYSDATETIMEOFFSET(), 5),20),Severity
Order By Convert(varchar(13),RaisedDateTime +RIGHT(SYSDATETIMEOFFSET(), 5),20)</CommandText>
      </Query>
      <Fields>
        <Field Name="RaisedDateTime">
          <DataField>RaisedDateTime</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Severity">
          <DataField>Severity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberOfAlerts">
          <DataField>NumberOfAlerts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DBServers">
      <Query>
        <DataSourceName>OperationalDataBaseMain</DataSourceName>
        <CommandText>

SELECT * FROM (
SELECT * from (
SELECT bme.DisplayName, s.HealthState,  Right(Fullname, LEN(Fullname) - 27) as Server
FROM state AS s, BaseManagedEntity as bme 
WHERE s.basemanagedentityid = bme.basemanagedentityid AND s.monitorid IN (SELECT MonitorId FROM Monitor 
WHERE MonitorName = 'System.Health.EntityState') and FullName like 'Microsoft.Windows.Computer:%') as S2
where S2.Server in (
	select ServerName from (
	Select iif(CHARINDEX(';',fullname)&lt;=0,fullname, Left(fullname, CHARINDEX(';',fullname)-1)) as ServerName, DisplayName, HealthState From
	(Select Replace(bme.FullName,'Microsoft.SQLServer.Database:','') as FullName, bme.DisplayName, s.HealthState
	from BaseManagedEntity as BME, State as S 
	WHERE s.basemanagedentityid = bme.basemanagedentityid
	and FullName like 'Microsoft.SQLServer.Database%' 
	and Name like 'OperationsManager%') as DB 
) as S1
group by ServerName) ) as S4
join (select ServerName, DisplayName as DatabaseName from (
	Select iif(CHARINDEX(';',fullname)&lt;=0,fullname, Left(fullname, CHARINDEX(';',fullname)-1)) as ServerName, DisplayName, HealthState From
	(Select Replace(bme.FullName,'Microsoft.SQLServer.Database:','') as FullName, bme.DisplayName, s.HealthState
	from BaseManagedEntity as BME, State as S 
	WHERE s.basemanagedentityid = bme.basemanagedentityid
	and FullName like 'Microsoft.SQLServer.Database%' 
	and Name like 'OperationsManager%') as DB  ) as A1
	group by ServerName, DisplayName) as X4 on S4.Server=X4.ServerName</CommandText>
      </Query>
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HealthState">
          <DataField>HealthState</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Server">
          <DataField>Server</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseName">
          <DataField>DatabaseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
