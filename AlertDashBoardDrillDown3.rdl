  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>OperationsManagerDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AlertName">
            <Value>=Parameters!AlertName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerName">
            <Value>=Parameters!ServerName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select DisplayName,AlertName,AlertDescription,RaisedDateTime +RIGHT(SYSDATETIMEOFFSET(), 5)[RaisedDateTime], max (ResolutionState)[resolutionstate], ars.AlertGuid[AlertGuid]
 from Alert.vAlertResolutionState ars 
inner join Alert.vAlertDetail adt on ars.alertguid = adt.alertguid 
inner join Alert.vAlert alt on ars.alertguid = alt.alertguid
Inner Join vManagedEntity me on alt.ManagedEntityRowID = me.ManagedEntityRowID
Where RaisedDateTime &gt;= DateAdd(Month,-1,GetUTCDate())
and ResolutionState &lt; 255
And AlertName = @AlertName
And  DisplayName = @ServerName
Group By DisplayName,Path,AlertName,AlertDescription,RaisedDateTime +RIGHT(SYSDATETIMEOFFSET(), 5),ars.AlertGuid
Order By RaisedDateTime DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
